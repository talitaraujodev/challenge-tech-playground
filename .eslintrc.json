{
  "env": {
    "node": true,
    "jest": true,
    "es2020": true
  },
  "extends": ["standard", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "no-useless-constructor": "off"
  }
}

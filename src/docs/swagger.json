        "title": "API de Feedback de Funcionários",
        "description": "API para gerenciamento de feedbacks de funcionários em um sistema de avaliação organizacional",
        "contact": {
            "name": "Support",
            "email": "talitasantosdev@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "tags": [
        {
            "name": "Employee Feedback"
        }
    ],
    "paths": {
        "/api/v1/employee-feedback": {
            "get": {
                "summary": "Listar todos os feedbacks de funcionários",
                "description": "Retorna uma lista de feedbacks de funcionários com opções de filtro e paginação",
                "tags": ["Employee Feedback"],
                "parameters": [
                    {
                        "in": "query",
                        "name": "page",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "Número da página"
                    },
                    {
                        "in": "query",
                        "name": "limit",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "Limite de itens por página"
                    },
                    {
                        "in": "query",
                        "name": "department",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "Marketing",
                                "Financeiro",
                                "Recursos Humanos",
                                "Tecnologia",
                                "Administração",
                                "Comercial",
                                "Operações",
                                "Logística",
                                "Jurídico"
                            ]
                        },
                        "description": "Filtrar por departamento"
                    },
                    {
                        "in": "query",
                        "name": "jobRole",
                        "schema": {
                            "type": "string"
                        },
                        "description": "Filtrar por cargo"
                    },
                    {
                        "in": "query",
                        "name": "location",
                        "schema": {
                            "type": "string"
                        },
                        "description": "Filtrar por localização"
                    },
                    {
                        "in": "query",
                        "name": "gender",
                        "schema": {
                            "type": "string",
                            "enum": ["MALE", "FEMALE", "OTHER"]
                        },
                        "description": "Filtrar por gênero"
                    },
                    {
                        "in": "query",
                        "name": "companyTenure",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "Menos de 6 meses",
                                "6 meses a 1 ano",
                                "1 a 2 anos",
                                "2 a 3 anos",
                                "3 a 4 anos",
                                "4 a 5 anos",
                                "5 a 6 anos",
                                "6 a 7 anos",
                                "7 a 8 anos",
                                "8 a 9 anos",
                                "9 a 10 anos",
                                "10 a 12 anos",
                                "12 a 15 anos",
                                "15 a 20 anos",
                                "Mais de 20 anos"
                            ]
                        },
                        "description": "Filtrar por tempo de empresa"
                    },
                    {
                        "in": "query",
                        "name": "generation",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "Geração Alpha",
                                "Geração Z",
                                "Geração Y",
                                "Geração X",
                                "Baby Boomers"
                            ]
                        },
                        "description": "Filtrar por geração"
                    },
                    {
                        "in": "query",
                        "name": "dateFrom",
                        "schema": {
                            "type": "string",
                            "format": "date"
                        },
                        "description": "Data de início para filtro"
                    },
                    {
                        "in": "query",
                        "name": "dateTo",
                        "schema": {
                            "type": "string",
                            "format": "date"
                        },
                        "description": "Data de término para filtro"
                    },
                    {
                        "in": "query",
                        "name": "search",
                        "schema": {
                            "type": "string"
                        },
                        "description": "Busca textual"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de feedbacks de funcionários retornada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/EmployeeFeedback"
                                            }
                                        },
                                        "total": {
                                            "type": "integer"
                                        },
                                        "page": {
                                            "type": "integer"
                                        },
                                        "limit": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro de validação ou requisição inválida",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Recurso não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erro interno do servidor",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
